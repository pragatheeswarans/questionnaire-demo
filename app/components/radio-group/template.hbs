<ul role="radiogroup" data-test-id="radiogroup">
  {{#each @options as |option|}}
    <RadioButton
      @label={{get option this.labelProperty}}
      @value={{get option this.valueProperty}}
      @groupName={{this.groupName}}
      @groupValue={{@groupValue}}
      @onChange={{@onChange}}
      @labelClass="form-field-label option"
    />
  {{/each}}
</ul>
